                }
            ],
            "realmRoles": [
                "operator"
            ],
            "clientRoles": {}
        },
        {
            "username": "viewer",
            "enabled": true,
            "emailVerified": true,
            "firstName": "Viewer",
            "lastName": "User",
            "email": "viewer@heimdall.local",
            "credentials": [
                {
                    "type": "password",
